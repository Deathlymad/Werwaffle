<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_join_lobby4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.philip.werwaffle.activity.Join_lobby4">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/joinLobbytex1"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/textView3"
        android:layout_toRightOf="@+id/password"
        android:layout_toEndOf="@+id/password"
        android:layout_marginLeft="39dp"
        android:layout_marginStart="39dp"
        android:text="@string/JoinLobbyTxt" />

    <Button
        android:text="@string/goToWifiMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/joinLobbytex1"
        android:layout_alignLeft="@+id/joinLobbytex1"
        android:layout_alignStart="@+id/joinLobbytex1"
        android:layout_marginTop="50dp"
        android:id="@+id/joinLobbyButton" />

</RelativeLayout>
